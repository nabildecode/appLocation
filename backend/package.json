    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jade": "^1.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.10",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "sharp": "^0.33.2",
    "ts-node-dev": "^2.0.0",
    "tsc-files": "^1.1.4",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
