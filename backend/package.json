{
  "name": "ts-sql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node src/index.js",
    "start:local": "ts-node-dev ./src/index.ts",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts'",
    "migration:create": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:generate src/orm/migrations/entities -d src/orm/data-source.ts",
    "migration:seed": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/orm/data-source.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.0.1",
