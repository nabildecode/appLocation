    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/mysql": "^2.15.25",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
